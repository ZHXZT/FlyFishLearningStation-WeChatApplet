
/* 视频列表 */
.lessonList{
  margin: 0 20rpx;
}
.dyMain{
  height: 150rpx;
  display: flex;
  padding: 20rpx 0 ;
  border-bottom:1rpx solid rgba(0, 0, 0, 0.070); 
  justify-content: space-between;
}
.dyItem{
  /* background-color: violet; */
  width:80%;
  display: flex;
  /* width: 400rpx; */
  height: 150rpx;

}
.dyItem .pic{
  width: 230rpx;
  height: 150rpx;
}
.dyItem .pic image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.dyItem .txt{
margin-left: 10rpx;
  /* background-color: antiquewhite; */

  width: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.dyItem .title{
  font-size: 33rpx;
}
.dyItem .info{
  display: flex;
  flex-direction: row;
  /* justify-content: space-between; */
  font-size: 20rpx;
  color: #888;
}
.dyItem .info .info1{
  background-color: rgba(255, 186, 59, 0.199);
  color: rgb(248, 142, 3);
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  margin-right: 10rpx;
  padding: 1rpx 10rpx;
}


.delete{
  font-size: 30rpx;
  color: white;
  background-color: rgb(255, 153, 0);
  height: 60rpx;
 /* width: 150rpx; */
 width:20%;
  border-radius: 60rpx;
  margin-top: 45rpx;
  text-align: center;
  line-height: 60rpx;
  justify-content: center;
}